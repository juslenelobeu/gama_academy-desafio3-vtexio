{
  "footer": {
    "blocks": ["footer-layout.desktop", "footer-layout.mobile"]
  },
  "footer-layout.desktop": { 
    "children": [
      "flex-layout.row#footer"
    ]
  },
  "footer-layout.mobile": { 
    "children": [
      "flex-layout.row#footer"
    ]
  },
  "flex-layout.row#footer": { 
    "children": [
      "link.product#footer-account",
      "link.product#footer-challenges",
      "link.product#footer-shop",
      "link.product#footer-support"
    ],
    "props": {
      "blockClass": "footer"
    }
  },
  "link.product#footer-account": {
    "props": {
      "href": "/account",
      "label": "Entrar",
      "displayMode": "button",
      "blockClass": "footerLink"
