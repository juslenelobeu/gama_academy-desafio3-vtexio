  },
  "link.product#footer-challenges": {
    "props": {
      "href": "/",
      "label": "Desafios",
      "displayMode": "button",
      "blockClass": "footerLink"
    }
  },
  "link.product#footer-shop": {
    "props": {
      "href": "/",
      "label": "Loja",
      "displayMode": "button",
      "blockClass": "footerLink"
    }
  },
  "link.product#footer-support": {
    "props": {
      "href": "https://api.whatsapp.com/send?l=pt-BR&phone=5511964335064&text=Para%20falar%20com%20a%20Gabi%20Clique%20enviar",
      "target": "_blank",
      "label": "Suporte",
      "displayMode": "button",
      "blockClass": "footerLink"
    }
  }
